{
  "compilerOptions": {
    "target": "es2016",                                  
    "module": "commonjs",                                
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true,                                      
    "skipLibCheck": true,
    "outDir": "./dist/",
    "rootDir": "./src/"
  }
}
